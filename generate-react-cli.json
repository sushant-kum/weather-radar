{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true,
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateName.module.scss",
        "test": "templates/component/TemplateName.test.tsx"
      }
    },
    "page": {
      "path": "src/pages",
      "withLazy": true,
      "withStory": false,
      "withStyle": true,
      "withTest": true,
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx",
        "lazy": "templates/page/TemplateName.lazy.tsx",
        "style": "templates/page/TemplateName.module.scss",
        "test": "templates/page/TemplateName.test.tsx"
      }
    },
    "layout": {
      "path": "src/layout",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true,
      "customTemplates": {
        "component": "templates/layout/TemplateName.tsx",
        "style": "templates/layout/TemplateName.module.scss",
        "test": "templates/layout/TemplateName.test.tsx"
      }
    }
  },
  "usesStyledComponents": true
}
